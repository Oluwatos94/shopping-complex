{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "jsx": "react",
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "paths": {
        "@/*": ["resources/ts/*"]
        }
    },
    "include": ["resources/ts/**/*"],
    "exclude": ["node_modules"]
}
